{
  "name": "@aiswelcome/worker",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc -b && cp -r src/templates dist/",
    "dev": "wrangler dev --local",
    "deploy": "wrangler deploy",
